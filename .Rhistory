# Initialize matrix to hold everyone's data
all_fits_wFree=matrix(nrow=10,ncol=5)
# Set number of iterations and parameters, want to use more than this for having a better outcome
num_iter=3
num_param=3
# Loop through subjects
for (t in 1:length(unique(dat$ID))){
# Set  starting values for each iteration.
# Can aim to cover parameter space evenly
starting_values <- matrix( runif(num_iter*num_param), nrow = num_iter)
opt <- apply(starting_values,1,function(x) optim(par=c(runif(3)),fn=map_trait,
data=subset(dat,(ID==t)),hessian=TRUE))
# Find the best fit out of these iterations
best_opt=opt[[which.min(unlist(lapply(opt,function(x) x$value)))]]
# Calculate log model evidence for best-fitting parameters
best_opt$lme=-1*best_opt$value + (length(best_opt$par)/2)*log(2*pi) -
(.5*log(det(best_opt$hessian)))
# Concatenate data across subjects
all_fits_wFree[t,]=c(best_opt$par,best_opt$value, best_opt$lme)
cat("Finished subject ", t,"\n")
}
