condition_label = c("Acceptance", "Rejection")
), aes(x = value_diff, y = predicted_prob, color = condition_label),
size = 3, shape = 21, fill = "white") +
scale_color_manual(
values = c("Acceptance" = "#88CCEE",
"Rejection" = "#FF6F61")
) +
