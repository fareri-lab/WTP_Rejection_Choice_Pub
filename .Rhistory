# Function to calculate Standard Error of the Mean (SEM)
safe_se <- function(x) {
if (length(na.omit(x)) > 1) {
return(sd(x, na.rm = TRUE) / sqrt(sum(!is.na(x))))
} else {
return(NA)
}
}
